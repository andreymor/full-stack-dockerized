            <section class="sidebar">
                <app-nav-menu menu-data="appMenu"></app-nav-menu>
            </section>
        </aside>

        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">

                <div app-breadcrumb></div>
                <app-alert></app-alert>
            </section>

            <!-- Main content -->
            <section class="content" ui-view>
