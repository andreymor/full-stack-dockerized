<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <title>ASAP</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <META HTTP-EQUIV="CACHE-CONTROL" CONTENT="NO-CACHE">

    {% block stylesheets %}
        {% stylesheets
        'vendor/bootstrap/bootstrap.css'
        'vendor/ng-table/ng-table.min.css'
        'vendor/app-utils/utils.min.css'
        'vendor/app-notify/notify.min.css'
        'vendor/app-translate/translate.min.css'
        'vendor/app-spinner/spinner.min.css'
        'vendor/app-datepicker/datepicker.min.css'
        'bundles/clientpublic/assets/css/AdminLTE.css'
        'bundles/clientpublic/assets/css/all_skins.css'
        'bundles/clientpublic/assets/css/dashboard.css'
        'bundles/clientpublic/assets/css/style.css'
        'bundles/clientpublic/assets/css/font-awesome-4.7.0/css/font-awesome.min.css'
        filter="cssrewrite" %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}

    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">


    <!-- SCRIPTS HACK IE -->
    <!--[if lt IE 9]>
    <script src="vendor/base/html5shiv.min.js"></script>
    <script src="vendor/base/respond.min.js"></script>
    <script>
        document.createElement('app-snap');
        document.createElement('accordion');
        document.createElement('accordion-group');
        document.createElement('accordion-heading');
        document.createElement('app-nav-menu');
        document.createElement('app-picklist');
        document.createElement('app-contador');
        document.createElement('app-usuario-info');
        document.createElement('app-identificador-ambiente');
        document.createElement('app-alert');
        document.createElement('timer');
        document.createElement('app-spinner');
        document.createElement('app-datepicker');
    </script>
    <![endif]-->

    <!-- Fav and touch icons -->
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/clientpublic/assets/images/favicon.png') }}"/>

</head>
{% verbatim %}
<body id="content" ng-class="{'login-page': !usuarioAutenticado, 'skin-blue fixed sidebar-mini' : usuarioAutenticado}" class="nojs ng-cloak hold-transition" ng-cloak>
