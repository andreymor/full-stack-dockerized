<script data-main="{{ asset('bundles/clientpublic/app/main.js?' ~ ("now"|date('YmdHis'))) }}"
        src="{{ asset('vendor/requirejs/require.min.js') }}"
        type="text/javascript"></script>

<script src="{{ asset('bundles/clientpublic/assets/js/jquery.min.js') }}"></script>
<script src="{{ asset('bundles/clientpublic/assets/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/clientpublic/assets/js/adminlte.min.js') }}"></script>

</body>
</html>
